min: D1 + D2 + D3;

e - 1 + 200 - 200 N1  >= D1;
1 - e + 200 N1        >= D1;

e - 2 + 200 - 200 N2  >= D2;
2 - e + 200 N2        >= D2;

e - 5 + 200 - 200 N3  >= D3;
5 - e + 200 N3        >= D3;

r1 + r2 + r3 = 1;

200 * r1 >=  D1;
200 * r2 >=  D2;
200 * r3 >=  D3;

bin N1, N2, N3;
bin r1, r2, r3;
